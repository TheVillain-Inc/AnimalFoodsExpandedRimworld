<?xml version="1.0" encoding="utf-8" ?>


<Defs>

<!-- WetPetFood -->

	<RecipeDef ParentName="CookMealBase">
		<defName>CookWetPetFood</defName>
		<label>cook wet pet food</label>
		<description>Cook a bowl of wet pet food from plant sources and raw meat.</description>
		<jobString>Cooking wet pet food.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
		</ingredients>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<products>
			<WetPetFood>10</WetPetFood>
		</products>
		<workSkill>Cooking</workSkill>
	</RecipeDef>

