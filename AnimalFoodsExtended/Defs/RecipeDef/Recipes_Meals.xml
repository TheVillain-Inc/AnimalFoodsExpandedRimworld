	<!-- Feedstock -->

	<RecipeDef ParentName="CookMealBase">
		<defName>CookFeedstockGrazing</defName>
		<label>prepare grazing feedstock</label>
		<description>Grind down hay and raw plants, then pellet it for long term storage.</description>
		<jobString>Prepare Grazing Feedstock.</jobString>
		<workAmount>520</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Hay</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<disallowedCategories>
				<li>MeatRaw</li>
				<li>AnimalProductRaw</li>
			</disallowedCategories>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<products>
			<Grazing_Feedstock>50</Grazing_Feedstock>
		</products>
		<workSkill>Cooking</workSkill>
		<skillRequirements>
      <Cooking>5</Cooking>
    </skillRequirements>
		<researchPrerequisite>Electricity</researchPrerequisite>
	</RecipeDef>

